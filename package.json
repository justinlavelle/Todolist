{
  "name": "todolist",
  "version": "0.27.7",
  "author": {
    "name": "blaadje",
    "email": "acharlot91@gmail.com"
  },
  "description": "Be productive quickly",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently -r -p \"{name}\" -n \"ðŸš‹ Webpack,ðŸ”Œ Electron\" -c \"green.bold,yellow.bold\" \"webpack-dev-server --config=./config/dev/webpack.config.js --mode development\" \"sleep 2 && electron ./src/main.js\"",
    "foo": "webpack-dev-server --color --progress --config=./config/dev/webpack.config.js --mode development",
    "build": "node config/prod/build.js",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "Todolist",
    "appId": "io.blaadje.todo-list",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons"
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.18.1",
    "lowdb": "^1.0.0",
    "universal-analytics": "^0.4.20",
    "v-click-outside": "^3.0.1",
    "vue": "^2.3.3",
    "vue-color": "^2.3.2",
    "vue-electron": "^1.0.6",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.10.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^8.0.6",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "concurrently": "^5.0.2",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.2.0",
    "electron-debug": "^1.1.0",
    "electron-log": "^4.2.0",
    "electron-updater": "^4.3.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.13.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.18.1",
    "svg-to-vue-component": "^0.3.8",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
